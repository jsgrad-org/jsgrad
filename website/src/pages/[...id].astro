---
import { App } from '../components/notebook/App'
import Layout from '../components/Layout.astro'
import { getCollection } from 'astro:content'

export const getStaticPaths = async () => {
  const data = await getCollection('notebooks')
  return data.map((x: any) => ({ params: { id: x.id === 'index' ? undefined : x.id }, props: x }))
}
const code = Astro.props.data.content
---

<Layout>
  <App code={code} client:only />
</Layout>
