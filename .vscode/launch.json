{
  "version": "0.2.0",
  "configurations": [
    {
      "request": "launch",
      "name": "Launch Program",
      "type": "node",
      "program": "${workspaceFolder}/test/ops.test.ts",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "deno",
      "runtimeArgs": [
        "test",
        "--inspect-wait",
        "--allow-all",
        "--no-check",
        "--filter",
        "op.sym"
      ],
      "attachSimplePort": 9229,
      "outputCapture": "std"
    }
  ]
}
